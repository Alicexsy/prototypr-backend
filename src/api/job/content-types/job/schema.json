{
  "kind": "collectionType",
  "collectionName": "job",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Job",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "url": {
      "type": "string"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "jobs"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "jobs"
    },
    "description": {
      "type": "richtext"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "fulltime",
        "parttime",
        "contract",
        "volunteer",
        "internship",
        "collaboration"
      ]
    },
    "salarymin": {
      "type": "integer",
      "max": 750000,
      "required": false
    },
    "salarymax": {
      "type": "integer",
      "max": 1000000
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "mappedBy": "jobs"
    },
    "expiry": {
      "type": "datetime"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-stripe.strapi-stripe-payment",
      "mappedBy": "job"
    },
    "promoted": {
      "type": "boolean"
    },
    "slug": {
      "type": "uid"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "jobs"
    }
  }
}
