{
  "kind": "collectionType",
  "collectionName": "sponsored_posts",
  "info": {
    "singularName": "sponsored-post",
    "pluralName": "sponsored-posts",
    "displayName": "Sponsorship",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "banner",
        "link",
        "website",
        "linkcombo",
        "bannercombo",
        "combo"
      ]
    },
    "tagline": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "sponsored_posts"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sponsored_posts"
    },
    "link": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "productIds": {
      "type": "string"
    },
    "paid": {
      "type": "boolean",
      "default": false
    },
    "paymentId": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "fallback": {
      "type": "boolean",
      "default": false
    },
    "cardImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "weeks": {
      "type": "json"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "sponsorships"
    }
  }
}
